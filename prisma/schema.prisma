generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Humidity {
  id         Int      @id @default(autoincrement())
  deviceId   String
  value      Decimal
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}

model Pressure {
  id         Int      @id @default(autoincrement())
  deviceId   String
  value      Decimal
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}

model Temperature {
  id         Int      @id @default(autoincrement())
  deviceId   String
  value      Decimal
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}